{
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "devcontainer",
  "workspaceFolder": "/workspace",
  "forwardPorts": [3000, 8123],
  "portsAttributes": {
  "3000": {
    "label": "ch-flow",
    "onAutoForward": "openBrowser"
  },
  "hostRequirements": {
    "cpus": 2,
    "memory": "8gb",
    "storage": "8gb"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-outside-of-docker": {}
  }
}
