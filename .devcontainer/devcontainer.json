{
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "devcontainer",
  "workspaceFolder": "/workspace",
  "forwardPorts": [3000, 8123, 9000],
  "hostRequirements": {
   "cpus": 2,
   "memory": "8gb",
   "storage": "8gb"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorTheme": "Default Dark+",
        "locale": "en"
      }
    }
  }
}
